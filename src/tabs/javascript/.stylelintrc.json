{
    "extends": ["stylelint-config-standard", "stylelint-config-recess-order"],
    "rules": {
        "block-no-empty": null,
        "indentation": 4,
        "string-quotes": "single",
        "number-leading-zero": "never",
        "unit-whitelist": ["px", "%", "deg", "s", "ms", "vh", "vw"],
        "at-rule-no-unknown": [true, {
            "ignoreAtRules": ["each", "include", "mixin", "for"]
        }]
    }
}
